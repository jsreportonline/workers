    "version": "0.2.0",
    "configurations": [{
         "type": "node",
         "env": {
            "connectionString:uri": "mongodb://localhost:27017/multitenant",
            "ip": "192.168.0.2",
            "workerUrls:t": "http://localhost:3000",
            "workerUrls:p": "http://localhost:4000",
            "workerUrls:f": "http://localhost:6000",
            "workerUrls:w": "http://localhost:5000",
            "workerUrls:h": "http://localhost:7000",
            "workerUrls:e": "http://localhost:8000",
            "stack": "dev",
            "containerRestartPolicy": "no"
         }, 
         "request": "launch",      
         "name": "Launch Program",
         "program": "${workspaceRoot}/index.js",
         "cwd": "${workspaceRoot}"       
    }]
}